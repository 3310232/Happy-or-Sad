{
  "extends": "@vue/tsconfig/tsconfig.json",
 "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "composite": false,
    "incremental": false,
    "baseUrl": ".",
    "allowJs": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
